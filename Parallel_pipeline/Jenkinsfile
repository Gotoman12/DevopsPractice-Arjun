pipeline {
    agent any
    stages{
        stage("First stage on window"){
            parallel{
                   stage("Checkin Code"){
                    steps{
                        sh '''
                        echo "checking code"
                        '''
                    }
                   }
                   stage("Checkin out"){
                    steps{
                        sh '''
                        echo "checking out"
                        '''
                    }
                   }
            }
        }
        stage("Deploy"){
            steps{
                sh '''
                   echo "Deploy code"
                '''
            }
        }
    }
}